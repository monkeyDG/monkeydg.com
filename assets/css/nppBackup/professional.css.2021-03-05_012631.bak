header,
nav {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	vertical-align: bottom;
	background: 0 0;
	display: block
}

header {
	background: #111;
	height: 62px;
	padding: 0 5%;
	width: 90%;
	position: absolute;
	z-index: 1;
	font-family: "Open Sans Regular", "Helvetica Neue", Helvetica, Arial, Sans-serif
}

header nav {
	display: none;
	background: #222;
	position: absolute;
	top: 62px;
	right: 0;
	z-index: 1000;
	width: 100%;
	font-size: 24px;
	font-size: 2.4rem;
	text-align: center
}

header nav li {
	height: 3em;
	list-style: none;
	display: block;
	border-top: #212121 solid 1px;
	border-bottom: #363636 solid 1px
}

header nav li a {
	display: block;
	color: #fff;
	line-height: 3em;
	text-decoration: none;
	text-transform: lowercase
}

header nav li a:hover {
	color: #66d6ff;
}


.logo {
	height: 80px;
	width: 192px;
	background: url(images/logo-initials-sprite-white0.png) no-repeat;
	transition: .1s;
	display: block;
	margin-right: 0;
	text-indent: -9999px;
	float: left;
}

.logo:hover {
	background: url(images/logo-initials-sprite-light2.png) no-repeat;
	transition: .1s
}

.icon-nav {
	height: 80px;
	width: 192px;
	float: right;
	background: url(images/logo-initials-sprite-white0.png) no-repeat;
	transition: .1s;
	display: block;
	text-indent: -9999px
}

.social li {
	display: inline-block;
	border: 0;
	margin-top: 18px;
	margin-left: 15px;
	height: 32px
}

.social li a {
	width: 32px;
	height: auto;
	display: inline-block;
	transition: .1s;
	text-indent: -9999px;
	cursor: pointer;
	padding: 0
}

.social li a:hover {
	width: 32px;
	height: auto;
	display: inline-block;
	transition: .1s;
	text-indent: -9999px;
	cursor: pointer;
	padding: 0
}

.social .github a {
	background: url(images/github-white.png);
	background-size: 32px 32px;
	background-repeat: no-repeat
}

.social .linkedin a {
	background: url(images/linkedin-white.png);
	background-size: 32px 32px;
	background-repeat: no-repeat
}

.social .github:hover a {
	background: url(images/github-blue.png);
	background-size: 32px 32px;
	background-repeat: no-repeat
}

.social .linkedin:hover a {
	background: url(images/linkedin-blue.png);
	background-size: 32px 32px;
	background-repeat: no-repeat
}


::selection {
	background: #333;
	color: #fff
}

::-moz-selection {
	background: #333;
	color: #fff
}

::-webkit-selection {
	background: #333;
	color: #fff
}

